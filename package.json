{
  "name": "freeapi",
  "version": "0.1.0",
  "description": "A tool that runs multiple AI web-based free chat services in the background",
  "main": "dist/index.js",
  "bin": {
    "freeapi": "./bin/freeapi"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "ai",
    "chat",
    "free",
    "api",
    "cli",
    "chatgpt",
    "deepseek",
    "wenxin",
    "qwen"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "inquirer": "^9.2.10",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "puppeteer": "^21.0.0",
    "playwright": "^1.40.0",
    "axios": "^1.6.2",
    "yaml": "^2.3.4",
    "winston": "^3.11.0",
    "conf": "^11.0.1",
    "fs-extra": "^11.2.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/inquirer": "^9.0.3",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/freeapi.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/freeapi/issues"
  },
  "homepage": "https://github.com/yourusername/freeapi#readme"
}